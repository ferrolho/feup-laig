		<node id="board-right">
			<transforms>
				<transform type="translate" to="-9.65 -0.95 0" />
				<transform type="rotate" axis="y" angle="90" />
				<transform type="rotate" axis="x" angle="90" />
				<transform type="scale" factor="20 0.7 1.4" />
			</transforms>

			<appearanceref id="wooden" />

			<primitives>
			</primitives>

			<descendants>
				<noderef id="unit-cube" />
