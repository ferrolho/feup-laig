		</node>

		<node id="board-drawer-1">
			<transforms>
			</transforms>

			<appearanceref id="drawer-wooden" />

			<animationref id="drawer-out-1" />

			<primitives>
			</primitives>

			<descendants>
				<noderef id="board-drawer" />
			</descendants>
		</node>

		<node id="board-drawer-2">
			<transforms>
				<transform type="rotate" axis="y" angle="180" />
