		</node>

		<node id="board-top">
			<transforms>
				<transform type="scale" factor="20 0.5 20" />
			</transforms>

			<appearanceref id="inherit" />

			<primitives>
			</primitives>

			<descendants>
				<noderef id="unit-cube" />
			</descendants>
		</node>

		<node id="board-bottom">
			<transforms>
				<transform type="translate" to="0 -2 0" />
				<transform type="scale" factor="20 0.7 20" />
			</transforms>

			<appearanceref id="wooden" />

			<primitives>
			</primitives>

			<descendants>
				<noderef id="unit-cube" />
